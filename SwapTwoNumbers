#include <stdio.h>

int main() {
    int a, b, temp;  // Declare three integer variables 'a', 'b', and 'temp'

    // Prompt the user to enter the values of 'a' and 'b'
    printf("Enter the first number (a): ");
    scanf("%d", &a);  // Read the value of 'a' from the user

    printf("Enter the second number (b): ");
    scanf("%d", &b);  // Read the value of 'b' from the user

    // Print the values before swapping
    printf("Before swapping: a = %d, b = %d\n", a, b);

    // Swap the values using a temporary variable 'temp'
    temp = a;  // Store the value of 'a' in 'temp'
    a = b;     // Assign the value of 'b' to 'a'
    b = temp;  // Assign the value of 'temp' (original 'a') to 'b'

    // Print the values after swapping
    printf("After swapping: a = %d, b = %d\n", a, b);

    return 0;  // Return 0 to indicate successful completion of the program
}
